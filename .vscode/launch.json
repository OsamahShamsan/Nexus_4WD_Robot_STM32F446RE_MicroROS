{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug STM32 (ST-LINK)",
            "cwd": "${workspaceFolder}",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stlink",
            "interface": "swd",
            "runToEntryPoint": "main",
            "device": "STM32F446RE",
            "executable": "${workspaceFolder}/Debug/NexusRobot_ROS2_Slave.elf",
            "svdFile": "${workspaceFolder}/STM32F446.svd",
            "armToolchainPath": "C:/ST/STM32CubeCLT_1.19.0/GNU-tools-for-STM32/bin",
            "gdbPath": "C:/ST/STM32CubeCLT_1.19.0/GNU-tools-for-STM32/bin/arm-none-eabi-gdb.exe",
            "serverpath": "C:/ST/STM32CubeCLT_1.19.0/STLink-gdb-server/bin/ST-LINK_gdbserver.exe",
            "serverArgs": [
                "-m", "0",
                "--swd",
                "-cp", "C:/Program Files/STMicroelectronics/STM32Cube/STM32CubeProgrammer/bin"
            ],
            "showDevDebugOutput": "raw"
        }
    ]
}
